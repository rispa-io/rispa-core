{
  "presets": [
    "react",
    "es2015",
    "es2016",
    "es2017"
  ],

  "plugins": [
    ["transform-runtime", { "moduleName": "babel-runtime" }],
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name",
    "transform-object-rest-spread",
    "transform-class-properties",
    "dynamic-import-webpack"
  ]
}